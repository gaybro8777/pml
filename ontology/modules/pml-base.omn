    Annotations: 
        <http://provenanceweb.org/ns/pml#order> "20.2"^^xsd:decimal,
        rdfs:comment "language used by the target."@en,
        rdfs:label "to-language"@en,
        <http://www.w3.org/ns/prov#category> "base"^^rdf:PlainLiteral
    
    Domain: 
        <http://provenanceweb.org/ns/pml#TranslationMethod>
    
    Range: 
        <http://provenanceweb.org/ns/pml#Language>
    
    
