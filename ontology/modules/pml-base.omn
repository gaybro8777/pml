    Annotations: 
        <http://provenanceweb.org/ns/pml#order> "20.1"^^xsd:decimal,
        rdfs:comment "language used by the source."@en,
        rdfs:label "from-language"@en,
        <http://www.w3.org/ns/prov#category> "base"^^rdf:PlainLiteral
    
    Domain: 
        <http://provenanceweb.org/ns/pml#TranslationMethod>
    
    Range: 
        <http://provenanceweb.org/ns/pml#Language>
    
    
