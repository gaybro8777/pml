<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY terms "http://purl.org/dc/terms/" >
    <!ENTITY prov "http://www.w3.org/ns/prov#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY pml "http://provenanceweb.org/ns/pml#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://provenanceweb.org/ns/pml#"
     xml:base="http://provenanceweb.org/ns/pml"
     xmlns:pml="http://provenanceweb.org/ns/pml#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://provenanceweb.org/ns/pml#">
        <rdfs:label xml:lang="en">Provenance Markup Language (PML 3.0)</rdfs:label>
        <rdfs:comment xml:lang="en">The Provenance Markup Language (PML 3.0) is an OWL ontology that extends W3C&#39;s PROV-O with the best parts of PML 2.0.</rdfs:comment>
        <owl:versionInfo xml:lang="en">version 3.0, Authors: Timothy Lebo, James P. McCusker, Paulo Pinheiro, Cynthia Chang, and Deborah L. McGuinness</owl:versionInfo>
        <owl:imports rdf:resource="http://www.w3.org/ns/inverses.owl"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/prov#"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&owl;versionInfo"/>
    <owl:AnnotationProperty rdf:about="&prov;category"/>
    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
    <owl:AnnotationProperty rdf:about="&prov;editorialNote"/>
    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://provenanceweb.org/ns/pml#authorOf -->

    <owl:ObjectProperty rdf:about="&pml;authorOf">
        <owl:inverseOf rdf:resource="&pml;hasAuthor"/>
        <rdfs:subPropertyOf rdf:resource="&prov;contributed"/>
    </owl:ObjectProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasAuthor -->

    <owl:ObjectProperty rdf:about="&pml;hasAuthor">
        <rdfs:range rdf:resource="&pml;Author"/>
        <rdfs:subPropertyOf rdf:resource="&prov;wasAttributedTo"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&pml;qualifiedAuthorship"/>
            <rdf:Description rdf:about="&prov;agent"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasDescription -->

    <owl:ObjectProperty rdf:about="&pml;hasDescription">
        <rdfs:label xml:lang="en">description</rdfs:label>
        <rdfs:comment xml:lang="en">The detailed description of the identified thing, e.g. a web page describing KIF language. An identified thing may have zero to many descriptions that could be written in different languages, format, and etc.</rdfs:comment>
        <rdfs:range rdf:resource="&pml;Information"/>
    </owl:ObjectProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasFormat -->

    <owl:ObjectProperty rdf:about="&pml;hasFormat">
        <rdfs:label xml:lang="en">format</rdfs:label>
        <rdfs:comment xml:lang="en">it specifies the format of information, e.g. &quot;pdf&quot;, &quot;ppt&quot; (Power Point). It is mainly used to determine the appropriate application for process/display purpose.</rdfs:comment>
        <rdfs:range rdf:resource="&pml;Format"/>
        <rdfs:domain rdf:resource="&pml;Information"/>
    </owl:ObjectProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasFromLanguage -->

    <owl:ObjectProperty rdf:about="&pml;hasFromLanguage">
        <rdfs:label xml:lang="en">from-language</rdfs:label>
        <rdfs:comment xml:lang="en">language used by the source.</rdfs:comment>
        <rdfs:range rdf:resource="&pml;Language"/>
        <rdfs:domain rdf:resource="&pml;TranslationMethod"/>
    </owl:ObjectProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasLanguage -->

    <owl:ObjectProperty rdf:about="&pml;hasLanguage">
        <rdfs:label xml:lang="en">language</rdfs:label>
        <rdfs:comment xml:lang="en">it specifies the language used to encode information, e.g. &quot;Tony&#39;s specialty is shellfish&quot; is encoded in English. This property may also link to a registered formal language such as KIF.</rdfs:comment>
        <rdfs:domain rdf:resource="&pml;Information"/>
        <rdfs:range rdf:resource="&pml;Language"/>
    </owl:ObjectProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasOwner -->

    <owl:ObjectProperty rdf:about="&pml;hasOwner">
        <rdfs:label xml:lang="en">owner</rdfs:label>
        <rdfs:comment xml:lang="en">the owner of a thing.</rdfs:comment>
        <rdfs:domain rdf:resource="&pml;OwnedEntity"/>
        <rdfs:range rdf:resource="&pml;Owner"/>
        <rdfs:subPropertyOf rdf:resource="&prov;wasAttributedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasPublisher -->

    <owl:ObjectProperty rdf:about="&pml;hasPublisher">
        <rdfs:label xml:lang="en">publisher</rdfs:label>
        <rdfs:comment xml:lang="en">An agent responsible for making the document publicly available. see also http://purl.org/dc/elements/1.1/publisher.</rdfs:comment>
        <rdfs:domain rdf:resource="&pml;Document"/>
        <rdfs:subPropertyOf rdf:resource="&prov;wasAttributedTo"/>
    </owl:ObjectProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasToLanguage -->

    <owl:ObjectProperty rdf:about="&pml;hasToLanguage">
        <rdfs:label xml:lang="en">to-language</rdfs:label>
        <rdfs:comment xml:lang="en">language used by the target.</rdfs:comment>
        <rdfs:range rdf:resource="&pml;Language"/>
        <rdfs:domain rdf:resource="&pml;TranslationMethod"/>
    </owl:ObjectProperty>
    


    <!-- http://provenanceweb.org/ns/pml#qualifiedAuthorship -->

    <owl:ObjectProperty rdf:about="&pml;qualifiedAuthorship">
        <rdfs:range rdf:resource="&pml;Authorship"/>
        <rdfs:subPropertyOf rdf:resource="&prov;qualifiedAttribution"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/isPartOf -->

    <owl:ObjectProperty rdf:about="&terms;isPartOf"/>
    


    <!-- http://www.w3.org/ns/prov#agent -->

    <owl:ObjectProperty rdf:about="&prov;agent"/>
    


    <!-- http://www.w3.org/ns/prov#agentOfInfluence -->

    <owl:ObjectProperty rdf:about="&prov;agentOfInfluence"/>
    


    <!-- http://www.w3.org/ns/prov#atLocation -->

    <owl:ObjectProperty rdf:about="&prov;atLocation"/>
    


    <!-- http://www.w3.org/ns/prov#contributed -->

    <owl:ObjectProperty rdf:about="&prov;contributed"/>
    


    <!-- http://www.w3.org/ns/prov#entity -->

    <owl:ObjectProperty rdf:about="&prov;entity"/>
    


    <!-- http://www.w3.org/ns/prov#hadRole -->

    <owl:ObjectProperty rdf:about="&prov;hadRole"/>
    


    <!-- http://www.w3.org/ns/prov#qualifiedAttribution -->

    <owl:ObjectProperty rdf:about="&prov;qualifiedAttribution"/>
    


    <!-- http://www.w3.org/ns/prov#wasAttributedTo -->

    <owl:ObjectProperty rdf:about="&prov;wasAttributedTo"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://provenanceweb.org/ns/pml#hasCreationDateTime -->

    <owl:DatatypeProperty rdf:about="&pml;hasCreationDateTime">
        <rdfs:label xml:lang="en">created</rdfs:label>
        <rdfs:comment xml:lang="en">When the artifact has been first created.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
        <rdfs:domain rdf:resource="&prov;Bundle"/>
        <rdfs:subPropertyOf rdf:resource="&prov;generatedAtTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasEncoding -->

    <owl:DatatypeProperty rdf:about="&pml;hasEncoding">
        <rdfs:label xml:lang="en">encoding</rdfs:label>
        <rdfs:comment xml:lang="en">The content encoding, e.g. &quot;base64&quot;. The value of this property helps applications determine the decoder of the raw string.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain rdf:resource="&prov;Entity"/>
    </owl:DatatypeProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasFromCol -->

    <owl:DatatypeProperty rdf:about="&pml;hasFromCol">
        <rdfs:label xml:lang="en">from-column</rdfs:label>
        <rdfs:comment xml:lang="en">The from column of the fragment (inclusive, index based on 1)</rdfs:comment>
        <rdfs:domain rdf:resource="&pml;RowColLocation"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasFromOffset -->

    <owl:DatatypeProperty rdf:about="&pml;hasFromOffset">
        <rdfs:label xml:lang="en">from-offset</rdfs:label>
        <rdfs:comment xml:lang="en">The from offset of the fragment (inclusive, index based on 0).</rdfs:comment>
        <rdfs:domain rdf:resource="&pml;OffsetPosition"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasFromRow -->

    <owl:DatatypeProperty rdf:about="&pml;hasFromRow">
        <rdfs:label xml:lang="en">from-row</rdfs:label>
        <rdfs:comment xml:lang="en">The from row of the fragment (inclusive, index based on 1)</rdfs:comment>
        <rdfs:domain rdf:resource="&pml;RowColLocation"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasModificationDateTime -->

    <owl:DatatypeProperty rdf:about="&pml;hasModificationDateTime">
        <rdfs:label xml:lang="en">last modified</rdfs:label>
        <rdfs:comment xml:lang="en">When the artifact has been modified.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
        <rdfs:domain rdf:resource="&prov;Bundle"/>
        <rdfs:subPropertyOf rdf:resource="&prov;generatedAtTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasPublicationDateTime -->

    <owl:DatatypeProperty rdf:about="&pml;hasPublicationDateTime">
        <rdfs:label xml:lang="en">published in</rdfs:label>
        <rdfs:comment xml:lang="en">When the publication is published.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
        <rdfs:subPropertyOf rdf:resource="&prov;generatedAtTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasToCol -->

    <owl:DatatypeProperty rdf:about="&pml;hasToCol">
        <rdfs:label xml:lang="en">to-column</rdfs:label>
        <rdfs:comment xml:lang="en">The to column of the fragment (inclusive, index based on 1)</rdfs:comment>
        <rdfs:domain rdf:resource="&pml;RowColLocation"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasToOffset -->

    <owl:DatatypeProperty rdf:about="&pml;hasToOffset">
        <rdfs:label xml:lang="en">to-offset</rdfs:label>
        <rdfs:comment xml:lang="en">The to offset of the fragment (exclusive, index based on 0)</rdfs:comment>
        <rdfs:domain rdf:resource="&pml;OffsetPosition"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasToRow -->

    <owl:DatatypeProperty rdf:about="&pml;hasToRow">
        <rdfs:label xml:lang="en">to-row</rdfs:label>
        <rdfs:comment xml:lang="en">The to row of the fragment (inclusive, index based on 1)</rdfs:comment>
        <rdfs:domain rdf:resource="&pml;RowColLocation"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://provenanceweb.org/ns/pml#hasVersion -->

    <owl:DatatypeProperty rdf:about="&pml;hasVersion">
        <rdfs:label xml:lang="en">version</rdfs:label>
        <rdfs:comment xml:lang="en">the version number.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/prov#generatedAtTime -->

    <owl:DatatypeProperty rdf:about="&prov;generatedAtTime"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://provenanceweb.org/ns/pml#Author -->

    <owl:NamedIndividual rdf:about="&pml;Author">
        <rdf:type rdf:resource="&owl;Class"/>
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&pml;authorOf"/>
                        <owl:someValuesFrom rdf:resource="&owl;Thing"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&prov;agentOfInfluence"/>
                        <owl:someValuesFrom>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="&prov;hadRole"/>
                                <owl:hasValue rdf:resource="&pml;Author"/>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&prov;Agent"/>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
    </owl:NamedIndividual>
    


    <!-- http://provenanceweb.org/ns/pml#Authorship -->

    <owl:Class rdf:about="&pml;Authorship">
        <rdfs:subClassOf rdf:resource="&prov;Attribution"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&prov;atLocation"/>
                <owl:onClass rdf:resource="&pml;Position"/>
                <owl:maxQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#DeclarativeRule -->

    <owl:Class rdf:about="&pml;DeclarativeRule">
        <rdfs:label xml:lang="en">Declarative Rule</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Plan"/>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
        <rdfs:comment xml:lang="en">A declarative inference rule can be fully specified by patterns for premises and its conclusion.   For example, Modus Ponens, can be specified by the premise patterns ?A,  ?A -&amp;gt; ?B and the conclusion pattern ?B.</rdfs:comment>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#Document -->

    <owl:Class rdf:about="&pml;Document">
        <rdfs:label xml:lang="en">Document</rdfs:label>
        <rdfs:subClassOf rdf:resource="&pml;InformationContainer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pml;hasVersion"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
        <rdfs:comment xml:lang="en">A physical information container that is not actionable. They function like database.</rdfs:comment>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#DocumentFragment -->

    <owl:Class rdf:about="&pml;DocumentFragment">
        <rdfs:label xml:lang="en">Document Fragment</rdfs:label>
        <rdfs:subClassOf rdf:resource="&pml;InformationContainer"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&prov;atLocation"/>
                <owl:someValuesFrom rdf:resource="&prov;Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&terms;isPartOf"/>
                <owl:onClass rdf:resource="&pml;Document"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
        <rdfs:comment xml:lang="en">When a Document is treated as a one dimensional string, a fragment of the Document may be established with two offsets. The offsets are in units of characters (and not bytes because characters may be multiple bytes).</rdfs:comment>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#EmptyInformation -->

    <owl:Class rdf:about="&pml;EmptyInformation">
        <rdfs:label xml:lang="en">Empty Information</rdfs:label>
        <rdfs:subClassOf rdf:resource="&pml;Information"/>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
        <rdfs:comment xml:lang="en">An instance of information indicating no conclusion has been derived, i.e. empty set of statements. In this case, the rawstring may contain some english message indicating no answer.</rdfs:comment>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#Format -->

    <owl:Class rdf:about="&pml;Format">
        <rdfs:label xml:lang="en">Format</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
        <rdfs:comment xml:lang="en">A plan for the organization and arrangement of specified information. Examples: binary, text,  pdf, etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#InferenceEngine -->

    <owl:Class rdf:about="&pml;InferenceEngine">
        <rdfs:label xml:lang="en">Inference Engine</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;SoftwareAgent"/>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
        <rdfs:comment xml:lang="en">An inference engine represents an engine that is able to produce a justification for a conclusion. Note that the phrase &quot;inference engine&quot; applies not only to reasoners but also to other systems like search engines which may justify their answers by direct assertion</rdfs:comment>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#Information -->

    <owl:Class rdf:about="&pml;Information">
        <rdfs:label xml:lang="en">Information</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pml;hasLanguage"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pml;hasFormat"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
        <rdfs:comment xml:lang="en">A piece of information, e.g. a formula in logic languages, and an utterance/word/phrase/sentence/paragraph/article in natural language. It is used as a range of iw:hasContent. This class and two of its sub-classes enable users to specify four types of semantics  (i) Information Annotation - annotate just the format and language used by the content of information; (ii) Information With Content - additionally provide the content of information that is materialized as string without loading a web page; (iii) Information external - additionally provide the URL for fetching the content; and (iv) Information - with every thing including the content string and URL. The second semantics is especially useful when the content is short or mainly used locally. For example, when learning by instruction, the utterances used for learning can be stored within the PML document without being externally stored. By assigning URI reference to information, we can even use owl:sameAs to capture the equivalence of two pieces of information.</rdfs:comment>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#InformationContainer -->

    <owl:Class rdf:about="&pml;InformationContainer">
        <rdfs:label rdf:datatype="&xsd;string">Information Container</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <prov:editorialNote rdf:datatype="&xsd;string">Do we mean frbr:Item or frbr:Manifestation?</prov:editorialNote>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#Language -->

    <owl:Class rdf:about="&pml;Language">
        <rdfs:label xml:lang="en">Language</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
        <rdfs:comment xml:lang="en">The language used to encode the raw string, e.g. English, Knowledge Interchange Format (KIF) , and N3. Languages are in general registered at the core IWRegistry.</rdfs:comment>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#OffsetPosition -->

    <owl:Class rdf:about="&pml;OffsetPosition">
        <rdfs:label xml:lang="en">Offset Position</rdfs:label>
        <rdfs:subClassOf rdf:resource="&pml;Position"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pml;hasToOffset"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
        <rdfs:comment xml:lang="en"></rdfs:comment>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#Ontology -->

    <owl:Class rdf:about="&pml;Ontology">
        <rdfs:label xml:lang="en">Ontology</rdfs:label>
        <rdfs:subClassOf rdf:resource="&pml;Document"/>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
        <rdfs:comment xml:lang="en">An Ontology is a document that describes a vocabulary of terms and their correlations.</rdfs:comment>
        <rdfs:comment xml:lang="en">This is a proper superclass of owl:Ontology, since ontologies may be expressed in formalisms other than OWL.</rdfs:comment>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#OwnedEntity -->

    <owl:Class rdf:about="&pml;OwnedEntity">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&pml;hasOwner"/>
                <owl:onClass rdf:resource="&pml;Owner"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#Owner -->

    <owl:Class rdf:about="&pml;Owner">
        <rdfs:subClassOf rdf:resource="&prov;Agent"/>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
    </owl:Class>
    


    <!-- http://provenanceweb.org/ns/pml#Ownership -->

    <owl:Class rdf:about="&pml;Ownership">
        <rdfs:subClassOf rdf:resource="&prov;Attribution"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&prov;agent"/>
                <owl:allValuesFrom rdf:resource="&pml;Owner"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&prov;entity"/>
                <owl:allValuesFrom rdf:resource="&pml;OwnedEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <prov:category rdf:datatype="&xsd;string">base</prov:category>
    </owl:Class>
    


